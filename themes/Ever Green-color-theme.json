{
  "name": "Ever Green",
  "colors": {
    "activityBar.background": "#ffffff",
    "activityBar.border": "#c6627a88",
    "activityBar.foreground": "#000000",
    "diffEditor.insertedTextBackground": "#59886722",
    "diffEditor.removedTextBackground": "#c4626c22",
    "editor.background": "#ffffff",
    "editor.findMatchBackground": "#ffcdff88",
    "editor.findMatchHighlightBackground": "#b4eacf88",
    "editor.findRangeHighlightBackground": "#bf360c88",
    "editor.foreground": "#444444",
    "editor.lineHighlightBackground": "#f0f0e3",
    "editor.rangeHighlightBackground": "#edfced88",
    "editor.selectionBackground": "#82828255",
    "editor.wordHighlightBackground": "#e6ebf188",
    "editor.wordHighlightStrongBackground": "#d5dfe888",
    "editorCursor.foreground": "#bf360c88",
    "editorGroup.border": "#c6627a88",
    "editorGroupHeader.border": "#c6627a88",
    "editorGroupHeader.noTabsBackground": "#ffffff",
    "editorGroupHeader.tabsBackground": "#ffffff",
    "editorGroupHeader.tabsBorder": "#c6627a88",
    "editorIndentGuide.background": "#82828255",
    "editorLineNumber.activeForeground": "#828282",
    "editorLineNumber.foreground": "#82828288",
    "editorMarkerNavigationWarning.background": "#c99e00",
    "editorOverviewRuler.warningForeground": "#c99e00",
    "editorRuler.foreground": "#82828255",
    "editorWarning.foreground": "#c99e00",
    "editorWhitespace.foreground": "#82828255",
    "peekView.border": "#828282",
    "peekViewEditor.background": "#fefef5",
    "peekViewEditor.matchHighlightBackground": "#b4eacf88",
    "peekViewResult.matchHighlightBackground": "#b4eacf88",
    "sideBar.background": "#ffffff",
    "sideBar.border": "#c6627a88",
    "sideBarTitle.foreground": "#bbbbbb",
    "sideBySideEditor.horizontalBorder": "#c6627a88",
    "sideBySideEditor.verticalBorder": "#c6627a88",
    "statusBar.background": "#ffffff",
    "statusBar.border": "#c6627a88",
    "statusBar.foreground": "#444444",
    "tab.activeBackground": "#f6bec8",
    "tab.activeForeground": "#000000",
    "tab.border": "#c6627a88",
    "tab.inactiveBackground": "#ffffff",
    "tab.inactiveForeground": "#888888",
    "titleBar.activeBackground": "#ffffff",
    "titleBar.border": "#c6627a88",
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.block.comment"
      ],
      "settings": {
        "foreground": "#8aabcc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Comment Doc",
      "scope": [
        "storage.type.class.jsdoc",
        "variable.other.jsdoc",
        "punctuation.definition.block.tag.jsdoc"
      ],
      "settings": {
        "foreground": "#8aabcc",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#a6559d",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#f091a0"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "variable.language"
      ],
      "settings": {
        "foreground": "#3271ae",
        "fontStyle": "bold"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#3271ae",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.other.property",
        "support.variable.property"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#31322c"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#f091a0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword control",
      "scope": [
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.logical.python"
      ],
      "settings": {
        "foreground": "#f091a0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword operator",
      "scope": [
        "keyword.operator",
        "punctuation.definition.block",
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#f091a0",
        "fontStyle": ""
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3271ae"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#cb523e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#84a729"
      }
    },
    {
      "name": "Function argument",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#a6559d",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag name",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#f091a0"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#b7d332"
      }
    },
    {
      "name": "Library function",
      "scope": [
        "support.function"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#4a8f8f"
      }
    },
    {
      "name": "Library constant",
      "scope": [
        "support.constant"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#3271ae"
      }
    },
    {
      "name": "Library variable",
      "scope": [
        "support.other.variable"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": ""
      }
    },
    {
      "name": "Invalid Illegal",
      "scope": [
        "invalid.illegal"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#828282"
      }
    },
    {
      "name": "Invalid Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#828282"
      }
    },
    {
      "name": "Property name",
      "scope": [
        "meta.object-literal.key",
        "support.type.property-name.json",
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#84769b"
      }
    },
    {
      "name": "HTML entitry",
      "scope": [
        "constant.character.entity.html"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#84769b"
      }
    },
    {
      "name": "Double quoted attribute",
      "scope": [
        "string.quoted.double.html"
      ],
      "settings": {
        "background": "",
        "foreground": "#598867"
      }
    },
    {
      "name": "CSS Property",
      "scope": [
        "entity.name.tag.css",
        "meta.property-name.css",
        "support.type.property-name",
        "support.type.vendored.property-name"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#455a64"
      }
    },
    {
      "name": "CSS Selector",
      "scope": [
        "meta.selector.css",
        "source.css entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#84769b"
      }
    },
    {
      "name": "CSS Value",
      "scope": [
        "support.constant.property-value.css",
        "support.constant.property-value.scss"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#598867"
      }
    },
    {
      "name": "CSS Tag",
      "scope": [
        "entity.name.tag.css"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#57749b"
      }
    },
    {
      "name": "CSS pseudo",
      "scope": [
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.pseudo-class.css"
      ],
      "settings": {
        "foreground": "#c4626c"
      }
    },
    {
      "name": "CSS unit",
      "scope": [
        "source.css keyword.other.unit"
      ],
      "settings": {
        "foreground": "#598867"
      }
    },
    {
      "name": "Media constant",
      "scope": [
        "support.constant.media.css"
      ],
      "settings": {
        "foreground": "#598867"
      }
    },
    {
      "name": "Python self",
      "scope": [
        "variable.language.special.self.python"
      ],
      "settings": {
        "foreground": "#c99e00",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "markup.heading.markdown",
        "markup.bold.markdown"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.italic.markdown"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.link.inline.markdown",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#57749b"
      }
    },
    {
      "scope": [
        "markup.fenced_code.block.markdown",
        "markup.inline.raw.string.markdown"
      ],
      "settings": {
        "foreground": "#8d9baa"
      }
    }
  ]
}